{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "mutate": [
    "src/App.js",
    "!src/data.js",
    "src/components/**/*.js?(x)",
    "!src/**/*@(.test|.spec|Spec).js?(x)"
  ],
  "mutator": {
    "name": "javascript",
    "excludedMutations": [
      "ArithmeticOperator",
      "ArrayDeclaration",
      "ArrowFunction",
      "BlockStatement",
      "BooleanLiteral",
      "ConditionalExpression",
      "EqualityOperator",
      "LogicalOperator",
      "ObjectLiteral",
      "UnaryOperator",
      "UpdateOperator"
    ]
  },
  "testRunner": "jest",
  "thresholds": { "high": 100, "low": 100, "break": 100 },
  "reporters": [
    "progress",
    "clear-text"
  ],
  "coverageAnalysis": "off",
  "jest": {
    "projectType": "create-react-app"
  }
}